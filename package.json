{
  "name": "@dorilama/byos",
  "version": "0.0.0",
  "description": "Single interface for multiple signals",
  "keywords": [
    "signals"
  ],
  "author": "dorilama",
  "license": "ISC",
  "exports": {
    ".": {
      "types": "./src/index.d.ts",
      "import": "./src/index.js"
    },
    "./@preact-signals-core": {
      "types": "./src/@preact-signals-core/index.d.ts",
      "import": "./src/@preact-signals-core/index.js"
    },
    "./@vue-reactivity": {
      "types": "./src/@vue-reactivity/index.d.ts",
      "import": "./src/@vue-reactivity/index.js"
    },
    "./@webreflection-signal": {
      "types": "./src/@webreflection-signal/index.d.ts",
      "import": "./src/@webreflection-signal/index.js"
    },
    "./alien-signals": {
      "types": "./src/alien-signals/index.d.ts",
      "import": "./src/alien-signals/index.js"
    },
    "./usignal": {
      "types": "./src/usignal/index.d.ts",
      "import": "./src/usignal/index.js"
    },
    "./package.json": "./package.json"
  },
  "module": "./src/index.js",
  "types": "./src/index.d.ts",
  "files": [
    "src"
  ],
  "homepage": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "scripts": {
    "test": "vitest",
    "install-peer": "node ./scripts/peer.js",
    "add-package": "node ./scripts/addPackage.js",
    "publish-package": "npm publish --access public"
  },
  "peerDependencies": {
    "@preact/signals-core": "^1",
    "@vue/reactivity": "^3",
    "@webreflection/signal": "^2",
    "alien-signals": "^1",
    "usignal": "^0.9"
  },
  "peerDependenciesMeta": {
    "@preact/signals-core": {
      "optional": true
    },
    "@vue/reactivity": {
      "optional": true
    },
    "@webreflection/signal": {
      "optional": true
    },
    "alien-signals": {
      "optional": true
    },
    "usignal": {
      "optional": true
    }
  },
  "type": "module",
  "devDependencies": {
    "@types/node": "^22.14.0",
    "vitest": "^3.1.1"
  }
}